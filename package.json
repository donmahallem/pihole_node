{
    "name": "pi1",
    "version": "1.0.0",
    "description": "tick",
    "main": "index.js",
    "scripts": {
        "test": "mocha --timeout 10000 --check-leaks -r ts-node/register src/**/*.spec.ts",
        "start": "npm run build:live",
        "build:live": "nodemon",
        "build": "node ./node_modules/typescript/bin/tsc --outdir ./dist/ --baseUrl ./src/",
        "build:doc": "node ./node_modules/typedoc/bin/typedoc --out ./tt2/ --mode modules --exclude /**/index ./src/",
        "test-ci_old": "nyc ts-mocha -p tsconfig.json -- --opts mocha.opts",
        "test-ci": "nyc mocha --opts mocha.opts"
    },
    "author": "",
    "license": "MIT",
    "dependencies": {
        "bcrypt": "~5.0.1",
        "body-parser": "~1.19.0",
        "cookie": "~0.4.1",
        "cookie-parser": "~1.4.5",
        "express": "~4.17.1",
        "helmet": "~4.6.0",
        "ini": "~2.0.0",
        "jsonschema": "~1.4.0",
        "jsonwebtoken": "~8.5.1",
        "moment": "~2.29.1",
        "rxjs": "~7.1.0",
        "sqlite3": "~5.0.2"
    },
    "devDependencies": {
        "@types/bcrypt": "5.0.0",
        "@types/chai": "4.2.18",
        "@types/chai-http": "4.2.0",
        "@types/cookie-parser": "1.4.2",
        "@types/express": "4.17.12",
        "@types/ini": "1.3.30",
        "@types/jsonschema": "1.1.1",
        "@types/jsonwebtoken": "8.5.1",
        "@types/mocha": "8.2.2",
        "@types/node": "14.17.2",
        "@types/serve-static": "1.13.9",
        "@types/sinon": "10.0.2",
        "@types/sqlite3": "3.1.7",
        "@types/supertest": "2.0.11",
        "chai": "4.3.4",
        "chai-http": "4.3.0",
        "istanbul": "0.4.5",
        "mocha": "9.0.0",
        "node-mocks-http": "1.10.1",
        "nodemon": "2.0.7",
        "nyc": "15.1.0",
        "sinon": "11.1.1",
        "sinon-chai": "3.7.0",
        "source-map-support": "0.5.19",
        "supertest": "6.1.3",
